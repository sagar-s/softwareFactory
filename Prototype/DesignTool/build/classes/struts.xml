<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
<package name="login" namespace="/" extends="struts-default">
	<action name="login" class="main.edu.asu.se.cdt.action.LoginAction">
			<result name="success">/WEB-INF/pages/addproject.jsp</result>
			<result name="login">/WEB-INF/pages/login.jsp</result>
			<result name="input">/WEB-INF/pages/login.jsp</result>
	</action>
</package>
<package name="addproject" namespace="/" extends="struts-default">
	<action name="addproject" class="main.edu.asu.se.cdt.action.AddProjectAction">
			<result name="success">/WEB-INF/pages/addusecase.jsp</result>
			<result name="login">/WEB-INF/pages/addproject.jsp</result>
			<result name="input">/WEB-INF/pages/addproject.jsp</result>
	</action>
</package>
<package name="addusecase" namespace="/" extends="struts-default">
	<action name="addusecase" class="main.edu.asu.se.cdt.action.AddUseCaseAction">
			<result name="success">/WEB-INF/pages/policiesandroles.jsp</result>
			<result name="login">/WEB-INF/pages/addusecase.jsp</result>
			<result name="input">/WEB-INF/pages/addusecase.jsp</result>
	</action>
</package>
<package name="policiesandroles" namespace="/" extends="struts-default">
	<action name="policiesandroles" class="main.edu.asu.se.cdt.action.MailDispatcherAction">
			<result name="success">/WEB-INF/pages/success.jsp</result>
			<result name="login">/WEB-INF/pages/addusecase.jsp</result>
			<result name="input">/WEB-INF/pages/addusecase.jsp</result>
	</action>
</package>
</struts>